<% interp = assertions.interpretation.first %>
<table class="table table-hover  border-left table-condensed">
  <thead>
    <tr>
      <th class="col-sm-10">Clinical validity of <%= gene.symbol %> and <%= disease.label %></th>
      <th class="col-sm-2 text-center"></th>
    </tr>
  </thead>
  <thead class="thead-labels">
    <tr>
      <th class="col-sm-10">Description</th>
      <th class="col-sm-2 text-center">Date</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><a href=""><strong><%= interp.label %></strong></a> association between <%= gene.symbol %> and <%= disease.label %>. </td>
      <td class=" text-center">
        <% if assertions.respond_to?(:date) %>
          <%= assertions.date %>
        <% else %>
          <%= assertions.first.date %>
        <% end %>
      </td>
    </tr>
  </tbody>
</table>
