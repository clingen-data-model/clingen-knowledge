<div id='condition1' name='condition1' class="row">
  <div class="col-sm-2">
    <h4><%= disease.label %></h4>
  </div>
  <div class="col-sm-10">
    <table class="table table-hover  border-left table-condensed">
      <thead>
        <tr>
          <th class="col-sm-10" colspan="2">Clinical validity of GENE1232 and Breast cancer</th>
          <th class="col-sm-2 text-center"><a class="btn btn-link btn-xs"><i class="glyphicon glyphicon-file"></i> View Details</a></th>
        </tr>
      </thead>
      <thead class="thead-labels">
        <tr>
          <th class="col-sm-5">Classifications</th>
          <th class="col-sm-5">Description</th>
          <th class="col-sm-2 text-center">Score</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>Gene-disease association</td>
          <td>Definitive association between GENE1232 and Breast cancer. </td>
          <td class=" text-center"><span class="label label-success" title="Jun 02, 2016">
            Definitive
          </span></td>
        </tr>
        
      </tbody>
    </table>
    <hr />
    <table class="table table-hover  border-left table-condensed">
      <thead>
        <tr>
          <th class="col-sm-10" colspan="2">Dosage Sensitivity of GENE1232 and Breast cancer</th>
          <th class="col-sm-2 text-center"><a class="btn btn-link btn-xs"><i class="glyphicon glyphicon-file"></i> View Details</a></th>
        </tr>
      </thead>
      <thead class="thead-labels">
        <tr>
          <th class="col-sm-5">Sensitivity Classifications</th>
          <th class="col-sm-5">Description</th>
          <th class="col-sm-2 text-center">Score</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>Haploinsufficiency</td>
          <td>Sufficient evidence for dosage pathogenicity</td>
          <td class=" text-center"><span class="label label-success" title="Jun 02, 2016">
            3
          </span></td>
        </tr>
        <tr>
          <td>Triplosensitivity</td>
          <td>No evidence available</td>
          <td class=" text-center"><span class="label label-success" title="Jun 02, 2016">
            0
          </span></td>
        </tr>
        
      </tbody>
    </table>
    <hr />
    <%= render "shared/actionability_detail", gene: @gene, disease: disease %>
  </div>
</div>
