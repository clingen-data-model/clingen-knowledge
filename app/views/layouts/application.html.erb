     ga('set', 'dimension5', '<%=@analyticsDimension5 %>');  
     //Queried Term
     <% end %>
     <% if @analyticsDimension6 %>
     ga('set', 'dimension6', '<%=@analyticsDimension6 %>');  
     //Non-curated Drug
     <% end %>
     <% if @analyticsDimension7 %>
     ga('set', 'dimension7', '<%=@analyticsDimension7 %>');  
     //Page type
     <% end %>
     ga('send', 'pageview');
    </script>


