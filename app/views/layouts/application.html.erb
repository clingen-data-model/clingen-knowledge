     ga('set', 'dimension2', '<%=@analyticsDimension2 %>');  
     //Curated Condition
     <% end %>
     <% if @analyticsDimension3 %>
     ga('set', 'dimension3', '<%=@analyticsDimension3 %>');  
     //Non-curated Gene
     <% end %>
