  </div>
<div id="header_highlight" class="container-fluid container-trans">  </div>
  
  <% if @notice.present? %>
    <div class="alert alert-warning" role="alert"><%= notice %></div>
  <% end %>
  <% if @alert.present? %>
    <div class="alert alert-danger" role="alert"><%= alert %></div>
  <% end %>
<div class="container search-content-header">
  <div class="row">
    <div class="col-sm-12">
      <div class="pull-right search-content-header-buttons ">
        <div class="btn-group" role="group" aria-label="...">
          <div class="btn-group" role="group">
            <button type="button" class="btn btn-default dropdown-hover" data-hover="dropdown" aria-haspopup="true" aria-expanded="false">
              ClinGen Curated <span class="caret"></span>
            </button>
            <ul class="dropdown-menu">
              <li>
                <%= link_to gene_disease_assertions_path, :class => ""  do %>
                  Gene Validity Curations
                <% end %>
              </li>
              <li>
                <%= link_to gene_dosage_index_path, :class => ""  do %>
                  Gene Dosage Curations
                <% end %>
              </li>
              <li>
                <%= link_to actionability_assertions_path, :class => ""  do %>
                  Actionability Curations
                <% end %>
              </li>
              <li role="separator" class="divider"></li>
              <li>
                <%= link_to conditions_path, :class => ""  do %>
                  Curated Conditions
                <% end %>
              </li>
              <li>
                <%= link_to genes_path, :class => ""  do %>
                  Curated Genes
                <% end %>
              </li>
            </ul>
          </div>
        <% if agent_signed_in? %>
          <div class="btn-group" role="group">
            <%= link_to dashboard_index_path, :class => "btn btn-default dropdown-toggle", :data => { :'hover' => 'dropdown' }  do %>
              <i class="glyphicon glyphicon-th-list"></i> <%= current_agent.first_name %>'s Dashboard <span class="caret"></span>
            <% end %>
            <ul class="dropdown-menu dropdown-menu-right">
              <li>
                <%= link_to dashboard_index_path, :class => ""  do %>
                  <i class="glyphicon glyphicon-th-list"></i> <%= current_agent.first_name %>'s Dashboard
                <% end %>
              </li>
              <li>
                <%= link_to edit_agent_registration_path, :class => ""  do %>
                  <i class="glyphicon glyphicon-user"></i> Manage Profile
                <% end %>
              </li>
              <li role="separator" class="divider"></li>
              <li>
                <%= link_to destroy_agent_session_path, :method => :delete, :class => ""  do %>
                  <i class="glyphicon glyphicon-off text-danger"></i> Sign-out
                <% end %>
              </li>
            </ul>
          </div>
        <% else %>
          <%= link_to agent_session_path, :method => :get, :class => "btn btn-default"  do %>
          <i class="glyphicon glyphicon-th-list"></i>
            <span class="">Knowledge-Base Sign-in</span>
          <% end %>
          <%= link_to new_agent_registration_path, :method => :get, :class => "btn btn-default"  do %>
          <i class="glyphicon glyphicon-bookmark"></i>
            <span class="">Sign-up</span>
          <% end %>
        <% end %>
      </div>
    </div>
    <h1>
    <%= link_to root_path, :method => :get, :class => "text-black"  do %>
          Knowledge-Base
    <% end %>
    </h1>
  </div>
  </div>
</div>
