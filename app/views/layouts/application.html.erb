          </div>
            <%= form_tag(home_index_path, method: "get") do %>
              <div id='header_search' class="input-group">
                <%= text_field_tag :term, "", class: 'form-control search-typeahead', placeholder: 'Search our knowledge-base for genes and diseases...' %>
                <span class="input-group-btn">
                  <button type="submit" class="btn btn-default"><i class="glyphicon glyphicon-search"></i></button>
                </span>
              </div>
            <% end %>
          <nav id='nav_primary' class=" hidden-sm hidden-xs">
              <ul class='nav nav-pills pull-right'>
          <li class='' role='presentation' id='ref_1001' >
            <a class='' href='http://clingen-pw3-v2.creationproject.net/about/' title='' >About</a>
