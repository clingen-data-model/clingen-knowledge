<div class="row">
  <% o[:interventions].each do |i| %>
  <div class="col-sm-5">
    <%= o[:label] %> / <%= i[:label] %>
  </div>
  <div class="col-sm-7">
    <dl class="dl-horizontal">
      <dt style="width:80px; font-weight:normal; margin-top:1px">Outcome</dt>
      <% o[:scores].each.with_index(0) do |s, key| %>
        <dd style="margin-left:90px">
          <%= (key == 0) ? 'Severity:' : 'Likelihood:' %>
          <strong>
            <%= s[:label] %>
          </strong>
        </dd>
      <% end %>
      <dt style="width:80px; font-weight:normal; margin-top:1px">Intervention</dt>
      <% i[:scores].each.with_index(0) do |s, key| %>
        <dd style="margin-left:90px">
          <%= (key == 0) ? 'Effectiveness:' : 'Intervention:' %>
          <strong>
            <%= s[:label] %>
          </strong>
        </dd>
      <% end %>
    </dl>
  </div>
  <% end %>
</div>
