<div class="row">
  <% o[:interventions].each do |i| %>
  <div class="col-sm-5">
    <%= o[:label] %> / <%= i[:label] %>
  </div>
  <div class="col-sm-7">
    <dl class="dl-horizontal">
      <% if o[:scores][0].present? %>
      <dt style="width:80px; font-weight:normal; margin-top:1px">Outcome</dt>
        <dd style="margin-left:90px">
          Severity:
          <strong>
            <%=o[:scores][1][:label] %>
          </strong>
        </dd>
        <dd style="margin-left:90px">
          Likelihood:
          <strong>
            <%=o[:scores][0][:label] %>
          </strong>
        </dd>
        <% end %>
      <% if i[:scores][0].present? %>
      <dt style="width:80px; font-weight:normal; margin-top:1px">Intervention</dt>
        <dd style="margin-left:90px">
          Effectiveness:
          <strong>
            <%=i[:scores][1][:label] %>
          </strong>
        </dd>
        <dd style="margin-left:90px">
          Nature of Intervention:
          <strong>
            <%=i[:scores][0][:label] %>
          </strong>
        </dd>
        <% end %>
    </dl>
  </div>
  <% end %>
</div>
